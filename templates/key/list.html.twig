{% extends 'base.html.twig' %}

{% block body_id 'key_list' %}
{% block body %}
    {{ form(form) }}
    <table class="table">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Key</th>
            <th scope="col">Product</th>

        </tr>
        </thead>

        <tbody>
        {% for key in keys %}
            <tr>
                <th scope="row">{{ key.id }}</th>
                <td>{{ key.key }}</td>
                <td>{{ key.product }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
